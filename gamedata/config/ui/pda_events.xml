<?xml version='1.0' encoding="UTF-8"?>
<!-- ЗАДАЧИ В КПК -->
<window>

	<main_wnd x="36" y="85" width="1000" height="620">

		<left_frame x="0" y="16" width="350" height="604" a="255">
			<texture>ui_pda_frame</texture>

			<primary_or_all x="18" y="430" width="270" height="449" check_mode="1" hint="pda_show_general_or_all_tasks">
				<texture>ui_PDA_checker</texture>
				<text align="l"  x="20" y="0" font="letterica16" color="white_4">show_only_general_tasks</text>
				<text_color_>
					<t color="white_4"/> <d color="grey_4"/> <e color="grey_2"/> <h color="white_2"/>
				</text_color_>
			</primary_or_all>

			<filter_tab x="6" y="3" width="293" height="48">
				<button x="5" y="0" width="70" height="24" hint="pda_btn_active_hint">
					<text align="l"  x="0" y="0" font="letterica18" color="white_4" complex_mode="0">pda_btn_active</text>
					<text_color>
						<t color="white_4"/> <d color="grey_4"/> <e color="grey_2"/> <h color="white_2"/>
					</text_color>
					<sound_h>device\pda\pda_tap_hover</sound_h>
					<sound_t>device\pda\pda_tap_active</sound_t>
				</button>
				<button x="80" y="0" width="100" height="24" hint="pda_btn_accomplished_hint">
					<text align="l"  x="0" y="0" font="letterica18" color="white_4" complex_mode="0">pda_btn_accomplished</text>
					<text_color>
						<t color="white_4"/> <d color="grey_4"/> <e color="grey_2"/> <h color="white_2"/>
					</text_color>
					<sound_h>device\pda\pda_tap_hover</sound_h>
					<sound_t>device\pda\pda_tap_active</sound_t>
				</button>
				<button x="185" y="0" width="60" height="24" hint="pda_btn_failed_hint">
					<text align="l"  x="0" y="0" font="letterica18" color="white_4" complex_mode="0">pda_btn_failed</text>
					<text_color>
						<t color="white_4"/> <d color="grey_4"/> <e color="grey_2"/> <h color="white_2"/>
					</text_color>
					<sound_h>device\pda\pda_tap_hover</sound_h>
					<sound_t>device\pda\pda_tap_active</sound_t>
				</button>
				<notused_button x="245" y="0" width="47" height="24" hint="pda_btn_own_hint">
					<text align="l"  x="0" y="0" font="letterica18" color="white_4" complex_mode="0">pda_btn_own</text>
					<text_color>
						<t color="white_4"/> <d color="grey_4"/> <e color="grey_2"/> <h color="white_2"/>
					</text_color>
					<sound_h>device\pda\pda_tap_hover</sound_h>
					<sound_t>device\pda\pda_tap_active</sound_t>
				</notused_button>
			</filter_tab>

			<left_frame_header x="1" y="29" width="348" height="27">
				<title x="0" y="0" width="280" height="27">
					  <text font="letterica18" color="transparent" complex_mode="0">Tasks</text>
				</title>
				<texture>ui_pda_line_h</texture>

				<anim_static x="316" y="-30" width="32" height="32" frames = "16" duration = "1000" columns = "8" frame_width = "32" frame_height = "32" cyclic = "1" autoplay = "1">
					<texture>ui\ui_pda_anims_01</texture>
				</anim_static>

			</left_frame_header>

			<list x="0" y="35" width="344" height="564" right_ident="10" always_show_scroll="1" scroll_profile="pda"/>
		</left_frame>

		<right_frame x="358" y="16" width="605" height="604">
			<task_descr_view x="0" y="0" width="588" height="558">

				<main_frame x="2" y="0" width="595" height="604" a="90">
					<texture>ui_pda_frame</texture>

					<header_frame_line x="1" y="29" width="593" height="27">
						<title x="0" y="-23" width="593" height="18">
							<text font="letterica18" color="grey_2" align="c" complex_mode="1">st_pda_task_detail_information</text>
						</title>
						<texture>ui_pda_line_h</texture>
					</header_frame_line>

					<scroll_view  x="16" y="35" width="573" height="563" scroll_profile="pda" always_show_scroll="1">
						<objective_item  y="0" width="800" height="10" >
							<image width="100" height="100" stretch="0"/>

							<text_cont  y="120" width="830" height="540">
								<text font="letterica18" color="white_1" complex_mode="1"/>
							</text_cont>

						</objective_item>
					</scroll_view>

				</main_frame>
			</task_descr_view>

			<map_wnd>

				<main_wnd x="0" y="0" width="606" height="604">

					<text_hint x="480" y="640" width="400" height="22" complex_mode="1">
						<text font="letterica18" align="r"/>
					</text_hint>

					<main_map_frame x="2" y="0" width="595" height="604" a="90">
						<texture>ui_pda_frame</texture>
							<level_frame x="8" y="8" width="558" height="568">
						</level_frame>
					</main_map_frame>

					<map_header_frame_line x="8" y="17" width="900" height="900">
						<texture>ui_pda_blank</texture>

						<tool_bar>
							<global_map_btn x="506" y="508" width="21" height="15" check_mode="0"  hint="global_map_btn_hint">
								<window_name>global_map_btn</window_name>
								<texture>zoom_off</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</global_map_btn>

							<actor_btn x="530" y="508" width="21" height="15" check_mode="0" hint="actor_btn_hint">
								<window_name>actor_btn</window_name>
								<texture>zoom_on</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</actor_btn>

							<zoom_out_btn x="506" y="532" width="17" height="14" check_mode="1" hint="zoom_out_btn_hint">
								<window_name>zoom_out_btn</window_name>
								<texture>zoom_out</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</zoom_out_btn>

							<zoom_in_btn x="530" y="532" width="17" height="14" check_mode="1" hint="zoom_in_btn_hint">
								<window_name>zoom_in_btn</window_name>
								<texture>zoom_in</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</zoom_in_btn>

							<highlight_spot_btn x="44" y="1" width="21" height="15" check_mode="1" hint="pda_show_pointer_btn_hint">
								<window_name>highlight_spot_btn</window_name>
								<texture>ui_icons_newPDA_mark</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</highlight_spot_btn>

							<add_spot_btn x="66" y="1" width="21" height="15" check_mode="1" hint="add_spot_btn_hint">
								<window_name>add_spot_btn</window_name>
								<texture>ui_icons_newPDA_perssign</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</add_spot_btn>

							<remove_spot_btn x="88" y="1" width="21" height="15" check_mode="1" hint="remove_spot_btn_hint">
								<window_name>remove_spot_btn</window_name>
								<texture>ui_icons_newPDA_perssigndel</texture>
								<sound_h>device\pda\pda_tap_hover</sound_h>
								<sound_t>device\pda\pda_tap_active</sound_t>
							</remove_spot_btn>
						</tool_bar>	

					</map_header_frame_line>
				</main_wnd>
			</map_wnd>
		</right_frame>
	</main_wnd>
</window>